{
  "name": "proxy-toggle",
  "displayName": "Proxy Toggle",
  "description": "Quickly switch between Windows proxy, WSL proxy, or disable",
  "version": "0.0.1",
  "publisher": "alireza-ghavabesh",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "proxyToggle.chooseProxy",
        "title": "Choose Proxy"
      }
    ],
    "keybindings": [
      {
        "command": "proxyToggle.chooseProxy",
        "key": "ctrl+alt+p",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "@vscode/test-electron": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alireza-Ghavabesh/adb-connector-vscode-extention.git"
  }
}